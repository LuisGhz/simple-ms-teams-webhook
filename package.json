    "typescript",
    "GitHub"
  ],
  "author": "notiz.dev",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "axios": "^0.20.0",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^12.7.12",
    "@zeit/ncc": "^0.20.5",
    "prettier": "^1.19.1",
    "typescript": "3.8.3"
  }
}
